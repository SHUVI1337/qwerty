   "source": [
    "from itertools import *\n",
    "def main():\n",
    "    n=int(input('колличество цифр в списке'))\n",
    "    a=input('цифры в списке')\n",
    "    b=list(a)\n",
    "    c=list(permutations(b,n))\n",
    "    z=[]\n",
    "    for i in range (n):\n",
    "        c1=list(c[i])\n",
    "        z.append(c1)\n",
    "    print(z)\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
