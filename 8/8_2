from itertools import *
def main(list1):
    list2=set()
    for i in list1:
        if i not in list2:
            list2.add(i)
    q=list()
    for l in range(1,10):
        q.extend(sorted(list(sorted(combinations(list2,l)))))
    q1=q.reverse()
    print(q1,len(q))
if __name__=='__main__':
    list1='abcdd'
    main(list1)
