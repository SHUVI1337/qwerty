def main():
    l=''
    s=''
    a=input()
    b=1
    m=0
    for i in range(len(a)):
        print(s)
        if a[i] in s:
            m=max(m,len(s))
            if len(a)>len(l):
                l=s
            s=''
            print(s)
        s = s + str(a[i])
    print(l)
if __name__=='__main__':
    main()
